<div>
  <h3><a href="<%= root_path %>" id="home">Developers</a></h3>
  <ul>
    <li class="dev">
      <a href="<%=root_path%>" >Back to all devs</a>
    </li>
  <% @devs.each do |dev| %>
    <li class="dev">
      <%= gravatar_for dev, :size => 25 %>
      <a class="dev_link" href="#" id="<%=dev.id%>"> <%= dev.user_github %></a>
    </li>
  <% end %>
    <li class="dev">
      <a href="#" class="new_element" id="new_developer" name="developers">+ Add new developer</a>
    </li>
  </ul>
</div>
